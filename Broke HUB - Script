local syde = loadstring(game:HttpGet("https://raw.githubusercontent.com/essencejs/syde/refs/heads/main/source", true))()

syde:Load({
	Logo = '7488932274',
	Name = 'Broke HUB',
	Status = 'Stable',
	Accent = Color3.fromRGB(251, 144, 255),
	HitBox = Color3.fromRGB(251, 144, 255),
	AutoLoad = false,
	Socials = {
		{
		}
	},
	ConfigurationSaving = {
		Enabled = true,
		FolderName = 'Broke HUB',
		FileName = "config"
	},
	AutoJoinDiscord = {
		Enabled = false,
		Invite = "Xf7hJj9RWd",
		RememberJoins = false
	}
})

local Window = syde:Init({
    Title = 'Broke HUB',
    SubText = 'By @IKenny'
})

local ButtonsTab  = Window:InitTab({ Title = 'Inicio' })
local ControlsTab = Window:InitTab({ Title = 'Local Player & Configurações' })
local AdvancedTab = Window:InitTab({ Title = 'Recursos avançados' })
local NeedScripts = Window:InitTab({ Title = 'Scripts Necessarios' })
local MuderMistery = Window:InitTab({ Title = 'Muder Mistery 2' })
local FleeTheFacility = Window:InitTab({ Title = 'Flee The Facility' })
local NineNineNights = Window:InitTab({ Title = '99 Nights In The Forest' })
local BladeBall = Window:InitTab({ Title = 'Blade Ball' })

ButtonsTab:Section('Teste de Executor')

ButtonsTab:Button({
	Title = 'UNC',
	Description = 'Mostra Quanto % seu executor consegue executar modulos de scripts',
	CallBack = function()
		loadstring(game:HttpGet("https://github.com/ltseverydayyou/uuuuuuu/blob/main/UNC%20test?raw=true"))()
		syde:Notify({
			Title = 'UNC - Test',
			Content = 'Teste Iniciado!',
			Duration = 2
		})
	end,
})

ButtonsTab:Button({
	Title = 'sUNC',
	Description = 'Projetado para elevar o padrão de testes de executores, garantindo que as funções operem conforme o esperado, expondo implementações falsas e fornecendo total transparência no desempenho do executor em situações reais.',
	CallBack = function()
		loadstring(game:HttpGet("https://script.sunc.su/"))()
		syde:Notify({
			Title = 'sUNC - Test',
			Content = 'Teste Iniciado!',
			Duration = 2
		})
	end,
}) 

